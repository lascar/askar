<html>
  <head>
    <title>Askar </title>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags  %>
  </head>
    <body>
      <div id="user_nav">
        Logged in as <%= current_user.email %>
        <%= link_to "Log out", log_out_path %>
      </div>
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash#{name}" %>
      <% end %>
  </body>
</html>
