/*jslint browser: true, nomen: true*/
/*global $, jQuery*/
Lascar.fields = <%= fields.to_json.html_safe %>;
Lascar.fields_to_show = <%= fields_to_show.to_json.html_safe %>;
Lascar.element_name = <%= element_name.to_json.html_safe %>;
Lascar.element = <%= element.to_json.html_safe %>;
Lascar.action = "show"

Lascar.show_init = function () {
  Lascar.displayTab(Lascar.action, Lascar.element_name);
}

Lascar.show_init();

